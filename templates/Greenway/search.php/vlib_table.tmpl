<div class="outer">
    <h2>Suchergebnis</h2>
    <div class="inner">
        Sie suchten nach "{TMPL_VAR NAME="keyword"}"
        <div style="float: right; display: inline;">
            <form action="export.php" method="post" style="display: inline;">
                <select name="format" size="1">
                    <option>XML</option>
                    <option>CSV</option>
                    <option>DokuWIKI</option>
                    <option>DymoCSV</option>
                </select>
                <input type='hidden' name='keyword' value='{TMPL_VAR NAME="keyword"}'>
                {TMPL_IF NAME="search_nam"}<input type='hidden' name='search_nam' value='true'>{/TMPL_IF}
                {TMPL_IF NAME="search_cat"}<input type='hidden' name='search_cat' value='true'>{/TMPL_IF}
                {TMPL_IF NAME="search_des"}<input type='hidden' name='search_des' value='true'>{/TMPL_IF}
                {TMPL_IF NAME="search_com"}<input type='hidden' name='search_com' value='true'>{/TMPL_IF} 
                {TMPL_IF NAME="search_sup"}<input type='hidden' name='search_sup' value='true'>{/TMPL_IF} 
                {TMPL_IF NAME="search_snr"}<input type='hidden' name='search_snr' value='true'>{/TMPL_IF} 
                {TMPL_IF NAME="search_loc"}<input type='hidden' name='search_loc' value='true'>{/TMPL_IF} 
                {TMPL_IF NAME="search_fpr"}<input type='hidden' name='search_fpr' value='true'>{/TMPL_IF} 
                <input type="submit" name="action" value="Export">
            </form>
        </div>
        <div class="clear"></div>
    </div>
</div>


<div class="outer">
    <div class="inner">
        <table>
            {TMPL_INCLUDE FILE="../vlib_table.tmpl"}
        </table>
    </div>
</div>
